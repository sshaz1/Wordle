<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wordle.GameResultController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="GAME RESULT" translateX="100.0" translateY="80.0">
            <font>
                <Font name="Helvetica Bold" size="44.0" />
            </font>
        </Text>
        <Button fx:id="levelSelectionButton" layoutX="200.0" layoutY="300.0" mnemonicParsing="false" onAction="#levelSelectionClicked" text="Level Selection" />
        <Label fx:id="gameScore" alignment="CENTER" layoutX="170.0" layoutY="174.0" prefHeight="26.0" prefWidth="167.0" textAlignment="CENTER">
            <font>
                <Font size="26.0" />
            </font>
        </Label>
        <Label fx:id="gameOutcome" alignment="CENTER" layoutX="120.0" layoutY="118.0" prefHeight="38.0" prefWidth="271.0" textAlignment="CENTER">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
